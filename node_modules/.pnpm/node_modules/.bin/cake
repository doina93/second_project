#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/coffee-script/1.3.3/node_modules/coffee-script/bin/cake/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/coffee-script/1.3.3/node_modules/coffee-script/bin/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/coffee-script/1.3.3/node_modules/coffee-script/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/coffee-script/1.3.3/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/coffee-script/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/node_modules:/mnt/c/SecondProject/second_project/node_modules:/mnt/c/SecondProject/node_modules:/mnt/c/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/node_modules/coffee-script/bin/cake/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/node_modules/coffee-script/bin/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/node_modules/coffee-script/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../coffee-script/bin/cake" "$@"
else 
  exec node  "$basedir/../coffee-script/bin/cake" "$@"
fi
