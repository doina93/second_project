#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/less/1.5.1/node_modules/less/bin/lessc/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/less/1.5.1/node_modules/less/bin/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/less/1.5.1/node_modules/less/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/less/1.5.1/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/less/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/node_modules:/mnt/c/SecondProject/second_project/node_modules:/mnt/c/SecondProject/node_modules:/mnt/c/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/node_modules/less/bin/lessc/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/node_modules/less/bin/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/node_modules/less/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../less/bin/lessc" "$@"
else 
  exec node  "$basedir/../less/bin/lessc" "$@"
fi
