#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/rimraf/2.2.8/node_modules/rimraf/bin.js/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/rimraf/2.2.8/node_modules/rimraf/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/rimraf/2.2.8/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/rimraf/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/node_modules:/mnt/c/SecondProject/second_project/node_modules:/mnt/c/SecondProject/node_modules:/mnt/c/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/node_modules/rimraf/bin.js/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/node_modules/rimraf/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rimraf/bin.js" "$@"
else 
  exec node  "$basedir/../rimraf/bin.js" "$@"
fi
