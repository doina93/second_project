#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/js-yaml/2.0.5/node_modules/js-yaml/bin/js-yaml.js/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/js-yaml/2.0.5/node_modules/js-yaml/bin/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/js-yaml/2.0.5/node_modules/js-yaml/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/js-yaml/2.0.5/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/js-yaml/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/node_modules:/mnt/c/SecondProject/second_project/node_modules:/mnt/c/SecondProject/node_modules:/mnt/c/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/node_modules/js-yaml/bin/js-yaml.js/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/node_modules/js-yaml/bin/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/node_modules/js-yaml/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../js-yaml/bin/js-yaml.js" "$@"
else 
  exec node  "$basedir/../js-yaml/bin/js-yaml.js" "$@"
fi
