#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/clean-css/2.0.8/node_modules/clean-css/bin/cleancss/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/clean-css/2.0.8/node_modules/clean-css/bin/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/clean-css/2.0.8/node_modules/clean-css/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/clean-css/2.0.8/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/clean-css/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/node_modules:/mnt/c/SecondProject/second_project/node_modules:/mnt/c/SecondProject/node_modules:/mnt/c/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../clean-css/2.0.8/node_modules/clean-css/bin/cleancss" "$@"
else 
  exec node  "$basedir/../../../../../../clean-css/2.0.8/node_modules/clean-css/bin/cleancss" "$@"
fi
