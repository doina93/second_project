#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/which/1.0.9/node_modules/which/bin/which/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/which/1.0.9/node_modules/which/bin/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/which/1.0.9/node_modules/which/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/which/1.0.9/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/which/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/node_modules:/mnt/c/SecondProject/second_project/node_modules:/mnt/c/SecondProject/node_modules:/mnt/c/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../which/1.0.9/node_modules/which/bin/which" "$@"
else 
  exec node  "$basedir/../../../../../../which/1.0.9/node_modules/which/bin/which" "$@"
fi
