#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/esprima/1.0.4/node_modules/esprima/bin/esvalidate.js/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/esprima/1.0.4/node_modules/esprima/bin/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/esprima/1.0.4/node_modules/esprima/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/esprima/1.0.4/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/esprima/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/SecondProject/second_project/node_modules/.pnpm/node_modules:/mnt/c/SecondProject/second_project/node_modules:/mnt/c/SecondProject/node_modules:/mnt/c/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../esprima/1.0.4/node_modules/esprima/bin/esvalidate.js" "$@"
else 
  exec node  "$basedir/../../../../../../esprima/1.0.4/node_modules/esprima/bin/esvalidate.js" "$@"
fi
